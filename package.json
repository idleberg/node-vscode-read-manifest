{
  "name": "vscode-read-manifest",
  "version": "0.3.1",
  "description": "Read the manifest (package.json) of any installed Visual Studio Code extension",
  "license": "MIT",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project ./",
    "test": "npm run lint"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-vscode-read-manifest.git"
  },
  "keywords": [
    "vscode",
    "vscode extension",
    "package.json",
    "package manifest"
  ],
  "dependencies": {
    "find-up": "^4.1.0",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "husky": "^3.0.1",
    "tslint": "^5.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}

{
  "name": "vscode-read-manifest",
  "version": "0.5.0",
  "description": "Read the manifest (package.json) of any installed Visual Studio Code extension",
  "license": "MIT",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "npm run start",
    "lint": "tslint --project ./",
    "start": "tsc --watch --pretty --project ./",
    "test": "npm run lint"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-vscode-read-manifest.git"
  },
  "keywords": [
    "vscode",
    "vscode extension",
    "package.json",
    "package manifest"
  ],
  "dependencies": {
    "caller-callsite": "^4.1.0",
    "find-up": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.4",
    "husky": "^4.2.5",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
